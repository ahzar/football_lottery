<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.azhar.lottery.dao.gamadao.AsiaGameLinesDao">
    <insert id="insertAsiaGameLines" parameterType="com.azhar.lottery.bean.game.AsiaGameLines" useGeneratedKeys="true" keyProperty="asisLotteryCorporationsID">

        insert into asia_lines(gameID,asisHomeTeamLines,asisVisitingTeamLines,asisHandicap,compName)
        values(#{gameID},#{asisHomeTeamLines},#{asisVisitingTeamLines},#{asisHandicap},#{compName})
    </insert>



    </mapper>